<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">

    <item android:state_pressed="true">
        <objectAnimator
            android:duration="@android:integer/config_shortAnimTime"
            android:propertyName="translationZ"
            android:valueFrom="@dimen/circle_button_elevation_high"
            android:valueTo="@dimen/circle_button_elevation_low"
            android:valueType="floatType"/>
    </item>
    <item>
        <objectAnimator
            android:duration="@android:integer/config_shortAnimTime"
            android:propertyName="translationZ"
            android:valueFrom="@dimen/circle_button_elevation_low"
            android:valueTo="@dimen/circle_button_elevation_high"
            android:valueType="floatType"/>
    </item>

</selector>